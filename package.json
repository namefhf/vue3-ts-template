{
  "name": "vue3-ts-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "vue",
    "vue3",
    "template",
    "vite",
    "typescript",
    "multi page",
    "mpa"
  ],
  "homepage": "https://github.com/namefhf/vue3-ts-template",
  "bugs": "https://github.com/namefhf/vue3-ts-template/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namefhf/vue3-ts-template.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint ./ --max-warnings 0",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint \"**/*.{vue,css,less,scss}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "lint:fix": "eslint ./ --max-warnings 0 --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "eslint-config-inspect": "npx @eslint/config-inspector@latest",
    "clean": "rimraf ./node_modules",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write",
      "stylelint --fix --allow-empty-input"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.68.0",
    "@tanstack/vue-query-devtools": "^5.68.0",
    "@types/lodash-es": "^4.17.12",
    "@vant/use": "^1.6.0",
    "@vueuse/core": "^12.8.2",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.1",
    "vant": "^4.9.18",
    "vconsole": "^3.15.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.16.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-info": "^1.2.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.2",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-optimize-exclude": "^0.0.1",
    "vite-plugin-qrcode": "^0.2.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-virtual-mpa": "^1.12.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.4.0"
  },
  "packageManager": "pnpm@10.6.3"
}
