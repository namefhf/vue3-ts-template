{
  "name": "vue3-ts-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "vue",
    "vue3",
    "template",
    "vite",
    "typescript",
    "multi page",
    "mpa"
  ],
  "homepage": "https://github.com/namefhf/vue3-ts-template",
  "bugs": "https://github.com/namefhf/vue3-ts-template/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namefhf/vue3-ts-template.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check:format": "prettier --check .",
    "lint:eslint": "eslint ./ --max-warnings 0",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint \"**/*.{vue,css,less}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "lint:fix": "eslint ./ --max-warnings 0 --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "eslint-config-inspect": "npx @eslint/config-inspector@latest",
    "clean": "rimraf ./node_modules",
    "test": "vitest",
    "create-page": "node scripts/createPage.mjs"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{less,css,html}": [
      "prettier --write",
      "stylelint --fix --allow-empty-input"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.79.0",
    "@tanstack/vue-query-devtools": "^5.79.0",
    "@types/lodash-es": "^4.17.12",
    "@vant/use": "^1.6.0",
    "@vueuse/core": "^13.3.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.2",
    "vant": "^4.9.19",
    "vconsole": "^3.15.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.15.29",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "enquirer": "^2.4.1",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.4",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.20.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-info": "^1.2.2",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-optimize-exclude": "^0.0.1",
    "vite-plugin-qrcode": "^0.2.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-virtual-mpa": "^1.12.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.4.0"
  },
  "packageManager": "pnpm@10.11.0"
}
