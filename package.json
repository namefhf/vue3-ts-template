{
  "name": "vue3-ts-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "build:sit": "vue-tsc --noEmit && vite build --mode sit",
    "preview": "vite preview",
    "lint:eslint": "eslint ./ --max-warnings 0",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint \"**/*.{vue,css,less,scss}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "lint:fix": "eslint ./ --max-warnings 0 --fix",
    "prepare": "husky install",
    "eslint-config-inspect": "npx @eslint/config-inspector@latest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.54.2",
    "@tanstack/vue-query-devtools": "^5.54.2",
    "@types/lodash-es": "^4.17.12",
    "@unocss/reset": "^0.62.3",
    "@vant/use": "^1.6.0",
    "@vueuse/core": "^11.0.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.2",
    "prettier": "^3.3.3",
    "unocss": "^0.62.3",
    "vant": "^4.9.4",
    "vue": "^3.5.3",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vant/auto-import-resolver": "^1.2.1",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.5",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "postcss-html": "^1.7.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.9.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-info": "^1.1.0",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-define-options": "^1.4.9",
    "vite": "^5.4.3",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-optimize-exclude": "^0.0.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-virtual-mpa": "^1.12.1",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vue-tsc": "^2.1.6"
  }
}
